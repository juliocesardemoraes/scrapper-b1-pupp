{
  "name": "b1-scrapper",
  "productName": "b1-scrapper",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": "src/index.js",
  "scripts": {
    "start": "javascript-obfuscator src/render.js --output src/output-obfuscated.js --dead-code-injection false && electron-forge start",
    "package": "electron-forge package",
    "make": "javascript-obfuscator src/render.js --output src/output-obfuscated.js --dead-code-injection false && electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "keywords": [],
  "author": "jc",
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {
        "ignore": [
          "\\/src/render.js$",
          "\\./src/render.js$"
        ]
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "tron_forge"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "dependencies": {
    "bulma-toast": "^2.4.4",
    "electron-squirrel-startup": "^1.0.0",
    "playwright": "^1.44.1"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.50",
    "@electron-forge/maker-deb": "^6.0.0-beta.50",
    "@electron-forge/maker-rpm": "^6.0.0-beta.50",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.50",
    "@electron-forge/maker-zip": "^6.0.0-beta.50",
    "devtron": "^1.4.0",
    "electron": "^31.0.1"
  }
}
